    "paths": {
        "/v1/hello": {
            "get": {
                "description": "Say hello to incoming name.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Hello"
                ],
                "summary": "Hello",
                "operationId": "v1.api.hello",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Your name",
                        "name": "name",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/main.helloResponse"
                        },
                        "headers": {
                            "request-id": {
                                "type": "string",
                                "description": "Request id for with uuid generator."
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/main.httpError"
                        },
                        "headers": {
                            "request-id": {
                                "type": "string",
                                "description": "Request id for with uuid generator."
                            }
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "main.helloResponse": {
            "type": "object",
            "properties": {
                "response": {
                    "type": "string",
                    "example": "hello user"
                }
            }
        },
        "main.httpError": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer",
                    "example": 400
                },
                "message": {
                    "type": "string",
                    "example": "status bad request"
                }
            }
        }
    },
    "securityDefinitions": {
        "BasicAuth": {
            "type": "basic"
        }
    }
